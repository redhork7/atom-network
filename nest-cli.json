{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/atom-network/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/atom-network/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/atom-network",
  "projects": {
    "atom-network": {
      "type": "application",
      "root": "apps/atom-network",
      "entryFile": "main",
      "sourceRoot": "apps/atom-network/src",
      "compilerOptions": {
        "tsConfigPath": "apps/atom-network/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "chefrigo": {
      "type": "application",
      "root": "apps/chefrigo",
      "entryFile": "main",
      "sourceRoot": "apps/chefrigo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chefrigo/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    }
  }
}